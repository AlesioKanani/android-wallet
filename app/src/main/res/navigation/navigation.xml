        tools:layout="@layout/fragment_change_address"
        android:id="@+id/changeAddressFragment"
        android:name="com.mw.beam.beamwallet.screens.change_address.ChangeAddressFragment"
        android:label="ChangeAddressFragment" >
        <argument
            android:name="isFromReceive"
            app:argType="boolean" />
    </fragment>
