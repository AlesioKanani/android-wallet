        tools:layout="@layout/fragment_send_confirmation"
        android:id="@+id/sendConfirmationFragment"
        android:name="com.mw.beam.beamwallet.screens.send_confirmation.SendConfirmationFragment"
        android:label="SendConfirmationFragment" >
        <argument
            android:name="sendAddress"
            app:argType="string" />
        <argument
            android:name="outgoingAddress"
            app:argType="string" />
        <argument
            android:name="sendAmount"
            app:argType="long" />
        <argument
            android:name="fee"
            app:argType="long" />
        <argument
            android:name="comment"
            app:argType="string"
            app:nullable="true" />
        <action
            android:id="@+id/action_sendConfirmationFragment_to_saveAddressFragment"
            app:destination="@id/saveAddressFragment"
            app:enterAnim="@anim/fade_in"
