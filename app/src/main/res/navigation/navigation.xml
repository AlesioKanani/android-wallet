<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/welcomeCreateFragment">
    <fragment
        android:id="@+id/welcomeCreateFragment"
        tools:layout="@layout/fragment_welcome_create"
        android:name="com.mw.beam.beamwallet.screens.welcome_screen.welcome_create.WelcomeCreateFragment"
        android:label="WelcomeCreateFragment" >
        <action
            android:id="@+id/action_welcomeCreateFragment_to_welcomeRestoreFragment"
            app:destination="@id/welcomeRestoreFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_welcomeCreateFragment_to_welcomeDescriptionFragment"
            app:destination="@id/welcomeDescriptionFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/welcomeOpenFragment"
        tools:layout="@layout/fragment_welcome_open"
        android:name="com.mw.beam.beamwallet.screens.welcome_screen.welcome_open.WelcomeOpenFragment"
        android:label="WelcomeOpenFragment" >
        <action
            android:id="@+id/action_welcomeOpenFragment_to_welcomeProgressFragment"
            app:destination="@id/welcomeProgressFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_left" />
    </fragment>
    <fragment
        android:id="@+id/welcomeRestoreFragment"
        tools:layout="@layout/fragment_welcome_restore"
        android:name="com.mw.beam.beamwallet.screens.welcome_screen.welcome_restore.WelcomeRestoreFragment"
        android:label="WelcomeRestoreFragment" >
        <action
            android:id="@+id/action_welcomeRestoreFragment_to_passwordFragment"
            app:destination="@id/passwordFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"  />
    </fragment>
    <fragment
        android:id="@+id/welcomeProgressFragment"
        tools:layout="@layout/fragment_welcome_progress"
        android:name="com.mw.beam.beamwallet.screens.welcome_screen.welcome_progress.WelcomeProgressFragment"
        android:label="WelcomeProgressFragment" >
        <argument
            android:name="pass"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="mode"
            app:argType="string" />
        <argument
            android:name="seed"
            app:argType="string[]"
            app:nullable="true" />
        <action
            android:id="@+id/action_welcomeProgressFragment_to_walletFragment"
            app:destination="@id/walletFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@+id/navigation" />
    </fragment>
    <fragment
        android:id="@+id/welcomeSeedFragment"
        tools:layout="@layout/fragment_welcome_seed"
        android:name="com.mw.beam.beamwallet.screens.welcome_screen.welcome_seed.WelcomeSeedFragment"
        android:label="WelcomeSeedFragment" >
        <action
            android:id="@+id/action_welcomeSeedFragment_to_welcomeConfirmFragment"
            app:destination="@id/welcomeConfirmFragment"
            app:popUpToInclusive="false"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/welcomeConfirmFragment"
        tools:layout="@layout/fragment_welcome_confirm"
        android:name="com.mw.beam.beamwallet.screens.welcome_screen.welcome_confirm.WelcomeConfirmFragment"
        android:label="WelcomeConfirmFragment" >
        <argument
            android:name="seed"
            app:argType="string[]" />
        <action
            android:id="@+id/action_welcomeConfirmFragment_to_passwordFragment"
            app:destination="@id/passwordFragment"
            app:popUpToInclusive="false"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"  />
        <action
            android:id="@+id/action_welcomeConfirmFragment_to_welcomeSeedFragment"
            app:destination="@id/welcomeSeedFragment"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@+id/welcomeDescriptionFragment" />
    </fragment>
    <fragment
        tools:layout="@layout/fragment_passwords"
        android:id="@+id/passwordFragment"
        android:name="com.mw.beam.beamwallet.screens.create_password.PasswordFragment"
        android:label="PasswordFragment" >
        <argument
            android:name="phrases"
            app:argType="string[]"
            app:nullable="true" />
        <argument
            android:name="mode"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="passChangeMode"
            app:argType="boolean"
            android:defaultValue="false" />
        <action
            android:id="@+id/action_passwordFragment_to_welcomeProgressFragment"
            app:destination="@id/welcomeProgressFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_left" />
        <action
            android:id="@+id/action_passwordFragment_to_welcomeSeedFragment"
            app:destination="@id/welcomeSeedFragment"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@+id/welcomeDescriptionFragment"
            app:popUpToInclusive="false" />
    </fragment>
    <fragment
        android:id="@+id/welcomeDescriptionFragment"
        tools:layout="@layout/fragment_welcome_description"
        android:name="com.mw.beam.beamwallet.screens.welcome_screen.welcome_description.WelcomeDescriptionFragment"
        android:label="WelcomeDescriptionFragment" >
        <action
            android:id="@+id/action_welcomeDescriptionFragment_to_welcomeSeedFragment"
            app:destination="@id/welcomeSeedFragment"
            app:popUpToInclusive="false"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"  />
    </fragment>
    <fragment
        android:id="@+id/walletFragment"
        tools:layout="@layout/fragment_wallet"
        android:name="com.mw.beam.beamwallet.screens.wallet.WalletFragment"
        android:label="WalletFragment" >
        <action
            android:id="@+id/action_walletFragment_to_addressesFragment"
            app:destination="@id/addressesFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_walletFragment_to_settingsFragment"
            app:destination="@id/settingsFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_walletFragment_to_utxoFragment"
            app:destination="@id/utxoFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_walletFragment_to_receiveFragment"
            app:destination="@id/receiveFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_walletFragment_to_sendFragment"
            app:destination="@id/sendFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_walletFragment_to_transactionDetailsFragment"
            app:destination="@id/transactionDetailsFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_walletFragment_to_proofVerificationFragment"
            app:destination="@id/proofVerificationFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        tools:layout="@layout/fragment_settings"
        android:name="com.mw.beam.beamwallet.screens.settings.SettingsFragment"
        android:label="SettingsFragment" >
        <action
            android:id="@+id/action_settingsFragment_to_editCategoryFragment"
            app:destination="@id/editCategoryFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_settingsFragment_to_categoryFragment"
            app:destination="@id/categoryFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_settingsFragment_to_checkOldPassFragment"
            app:destination="@id/checkOldPassFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_settingsFragment_to_languageFragment"
            app:destination="@id/languageFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_settingsFragment_to_ownerKeyVerificationFragment"
            app:destination="@id/ownerKeyVerificationFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out"/>
    </fragment>
    <fragment
        android:id="@+id/addressesFragment"
        tools:layout="@layout/fragment_addresses"
        android:name="com.mw.beam.beamwallet.screens.addresses.AddressesFragment"
        android:label="AddressesFragment" >
        <action
            android:id="@+id/action_addressesFragment_to_addressFragment"
            app:destination="@id/addressFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/utxoFragment"
        tools:layout="@layout/fragment_utxo"
        android:name="com.mw.beam.beamwallet.screens.utxo.UtxoFragment"
        android:label="UtxoFragment" >
        <action
            android:id="@+id/action_utxoFragment_to_utxoDetailsFragment"
            app:destination="@id/utxoDetailsFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/receiveFragment"
        tools:layout="@layout/fragment_receive"
        android:name="com.mw.beam.beamwallet.screens.receive.ReceiveFragment"
        android:label="ReceiveFragment" >
        <argument
            android:name="amount"
            app:argType="long"
            android:defaultValue="0L" />
        <action
            android:id="@+id/action_receiveFragment_to_changeAddressFragment"
            app:destination="@id/changeAddressFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right">
            <argument android:defaultValue="true"
                android:name="isFromReceive" />
        </action>
        <argument
            android:name="walletAddress"
            app:argType="com.mw.beam.beamwallet.core.entities.WalletAddress"
            app:nullable="true"
            android:defaultValue="@null" />
        <action
            android:id="@+id/action_receiveFragment_to_qrDialogFragment"
            app:destination="@id/qrDialogFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_receiveFragment_to_editCategoryFragment"
            app:destination="@id/editCategoryFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right">
            <argument android:defaultValue="@null"
                android:name="categoryId" />
        </action>
    </fragment>
    <fragment
        android:id="@+id/sendFragment"
        tools:layout="@layout/fragment_send"
        android:name="com.mw.beam.beamwallet.screens.send.SendFragment"
        android:label="SendFragment" >
        <argument
            android:name="address"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null" />
        <argument
            android:name="amount"
            app:argType="long"
            android:defaultValue="0L" />
        <action
            android:id="@+id/action_sendFragment_to_changeAddressFragment"
            app:destination="@id/changeAddressFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right">
            <argument android:defaultValue="false"
                android:name="isFromReceive" />
        </action>
        <action
            android:id="@+id/action_sendFragment_to_sendConfirmationFragment"
            app:destination="@id/sendConfirmationFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_sendFragment_to_editCategoryFragment"
            app:destination="@id/editCategoryFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right">
            <argument android:defaultValue="@null"
                android:name="categoryId" />
        </action>
    </fragment>
    <fragment
        android:id="@+id/transactionDetailsFragment"
        tools:layout="@layout/fragment_transaction_details"
        android:name="com.mw.beam.beamwallet.screens.transaction_details.TransactionDetailsFragment"
        android:label="TransactionDetailsFragment" >
        <action
            android:id="@+id/action_transactionDetailsFragment_to_paymentProofDetailsFragment"
            app:destination="@id/paymentProofDetailsFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <argument
            android:name="txId"
            app:argType="string" />
        <action
            android:id="@+id/action_transactionDetailsFragment_to_receiveFragment"
            app:destination="@id/receiveFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_transactionDetailsFragment_to_sendFragment"
            app:destination="@id/sendFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        tools:layout="@layout/fragment_proof_verification"
        android:id="@+id/proofVerificationFragment"
        android:name="com.mw.beam.beamwallet.screens.proof_verification.ProofVerificationFragment"
        android:label="ProofVerificationFragment" />
    <fragment
        tools:layout="@layout/fragment_payment_proof_details"
        android:id="@+id/paymentProofDetailsFragment"
        android:name="com.mw.beam.beamwallet.screens.payment_proof_details.PaymentProofDetailsFragment"
        android:label="PaymentProofDetailsFragment" >
        <argument
            android:name="paymentProof"
            app:argType="com.mw.beam.beamwallet.core.entities.PaymentProof" />
    </fragment>
    <fragment
        android:id="@+id/addressFragment"
        tools:layout="@layout/fragment_address"
        android:name="com.mw.beam.beamwallet.screens.address_details.AddressFragment"
        android:label="AddressFragment" >
        <argument
            android:name="walletAddress"
            app:argType="com.mw.beam.beamwallet.core.entities.WalletAddress" />
        <action
            android:id="@+id/action_addressFragment_to_transactionDetailsFragment"
            app:destination="@id/transactionDetailsFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_addressFragment_to_editAddressFragment"
            app:destination="@id/editAddressFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_addressFragment_to_qrDialogFragment"
            app:destination="@id/qrDialogFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
    </fragment>
    <fragment
        tools:layout="@layout/fragment_utxo_details"
        android:id="@+id/utxoDetailsFragment"
        android:name="com.mw.beam.beamwallet.screens.utxo_details.UtxoDetailsFragment"
        android:label="UtxoDetailsFragment" >
        <argument
            android:name="utxo"
            app:argType="com.mw.beam.beamwallet.core.entities.Utxo" />
    </fragment>
    <fragment
        tools:layout="@layout/fragment_edit_address"
        android:id="@+id/editAddressFragment"
        android:name="com.mw.beam.beamwallet.screens.address_edit.EditAddressFragment"
        android:label="EditAddressFragment" >
        <argument
            android:name="walletAddress"
            app:argType="com.mw.beam.beamwallet.core.entities.WalletAddress" />
        <action
            android:id="@+id/action_editAddressFragment_to_editCategoryFragment"
            app:destination="@id/editCategoryFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right">
            <argument android:defaultValue="@null"
                android:name="categoryId" />
        </action>
    </fragment>
    <fragment
        tools:layout="@layout/fragment_edit_category"
        android:id="@+id/editCategoryFragment"
        android:name="com.mw.beam.beamwallet.screens.edit_category.EditCategoryFragment"
        android:label="EditCategoryFragment" >
        <argument
            android:name="categoryId"
            app:argType="string"
            app:nullable="true" />
    </fragment>
    <fragment
        tools:layout="@layout/fragment_category"
        android:id="@+id/categoryFragment"
        android:name="com.mw.beam.beamwallet.screens.category.CategoryFragment"
        android:label="CategoryFragment" >
        <argument
            android:name="categoryId"
            app:argType="string" />
        <action
            android:id="@+id/action_categoryFragment_to_editCategoryFragment"
            app:destination="@id/editCategoryFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_categoryFragment_to_addressFragment"
            app:destination="@id/addressFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        tools:layout="@layout/fragment_check_old_pass"
        android:id="@+id/checkOldPassFragment"
        android:name="com.mw.beam.beamwallet.screens.check_old_pass.CheckOldPassFragment"
        android:label="CheckOldPassFragment" >
        <action
            android:id="@+id/action_checkOldPassFragment_to_passwordChangeFragment"
            app:destination="@id/passwordChangeFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@+id/checkOldPassFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        tools:layout="@layout/fragment_passwords"
        android:id="@+id/passwordChangeFragment"
        android:name="com.mw.beam.beamwallet.screens.create_password.PasswordFragment"
        android:label="PasswordFragment" >
        <argument
            android:name="phrases"
            app:argType="string[]"
            app:nullable="true" />
        <argument
            android:name="mode"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="passChangeMode"
            app:argType="boolean"
            android:defaultValue="false" />
    </fragment>
    <fragment
        android:id="@+id/languageFragment"
        tools:layout="@layout/fragment_language"
        android:name="com.mw.beam.beamwallet.screens.language.LanguageFragment"
        android:label="LanguageFragment" />
    <fragment
        android:id="@+id/ownerKeyVerificationFragment"
        tools:layout="@layout/fragment_owner_key_verification"
        android:name="com.mw.beam.beamwallet.screens.owner_key_verification.OwnerKeyVerificationFragment"
        android:label="OwnerKeyVerificationFragment" >
        <action
            android:id="@+id/action_ownerKeyVerificationFragment_to_ownerKeyFragment"
            app:destination="@id/ownerKeyFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@+id/settingsFragment"
            app:popUpToInclusive="false" />
    </fragment>
    <fragment
        android:id="@+id/ownerKeyFragment"
        tools:layout="@layout/fragment_owner_key"
        android:name="com.mw.beam.beamwallet.screens.owner_key.OwnerKeyFragment"
        android:label="OwnerKeyFragment" />
